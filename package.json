{
  "name": "almight",
  "private": true,
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@microsoft/tsdoc": "^0.13.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/chai": "^4.3.1",
    "@types/jest": "^27.4.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.30",
    "@types/web": "^0.0.61",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "chai": "^4.3.6",
    "corejs": "^1.0.0",
    "eslint": "^8.22.0",
    "glob": "^8.0.3",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "minimatch": "^5.1.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "lerna run tsc",
    "tsc": "tsc",
    "bootstrap": "lerna bootstrap --include-dependencies",
    "docs:start": "npm run start --prefix docs/",
    "docs:build": "npm run build --prefix docs/",
    "docs:serve": "npm run serve --prefix docs/",
    "lint": "npm eslint .",
    "test": "npx nyc@latest lern run test",
    "check": "npm run lint; npm rub build; npm run test",
    "reset": "lerna clean; npm run check",
    "new-version": "lerna version --no-git-tag-version --exact",
    "pre-publish": "npm run new-version; npm run reset",
    "npm-public:rc": "lerna exec -- npm public --access public --tag rc",
    "npm-publish:latest": "lerna exec -- npm publish --access public --tag latest",
    "npm-publish:next": "lerna exec -- npm publish --access public --tag next",
    "npm-publish:canary": "lerna exec -- npm publish --access public --tag canary",
    "coverage:combined": "lerna exec -- npm run coverage"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0"
  }
}
